{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Storage": {
    "ConnectionString": "UseDevelopmentStorage=true;DevelopmentStorageProxyUri=http://azurite",
    "ContainerName": "spreadsheets"
  },
  "Queue": {
    "ConnectionString": "UseDevelopmentStorage=true;DevelopmentStorageProxyUri=http://azurite",
    "QueueName": "upload"
  },
  "MongoDB": {
    "Database": "catalog",
    "ConnectionString": "mongodb://root:example@mongo:27017",
    "Host": "mongo",
    "Port": 27017,
    "User": "root",
    "Password": "example"
  }
}
